apiVersion: v1
kind: Service
metadata:
  name: proxy
  labels:
    name: proxy
  namespace: harbor
spec:
  type: NodePort
  ports:
    - name: http
      port: 80
      nodePort: {{ harbor_http_nodeport }}
    - name: http4ecp
      port: 8080
      nodePort: {{ harbor_http4ecp_nodeport }}
    - name: https
      port: 443
      nodePort: {{ harbor_https_nodeport }}
  selector:
    name: proxy
